unit Interfaces.Model.Classes.Users;

interface

uses
  System.Generics.Collections,
  System.JSON,
  Interfaces.Model.Classes.User;

type
  IModelClassUsers = interface
    function GetList: TList<IModelClassUser>;

    property List: TList<IModelClassUser> read GetList;

    procedure UpdateFromDataSet;
    procedure UpdateInDataSet;
    function ToJSON: TJSONArray;

    procedure Add(const AUser: IModelClassUser);
    function GetByGID(const AGID: String): IModelClassUser;
  end;

implementation

end.