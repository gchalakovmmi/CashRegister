unit Interfaces.Model.Classes.Items;

interface

uses
  System.Generics.Collections,
  System.JSON,
  Interfaces.Model.Classes.Item;

type
  IModelClassItems = interface
    function GetList: TList<IModelClassItem>;

    property List: TList<IModelClassItem> read GetList;

    procedure UpdateFromDataSet;
    procedure UpdateInDataSet;
    function ToJSON: TJSONArray;

    procedure Add(const AItem: IModelClassItem);
    function GetByGID(const AGID: String): IModelClassItem;
  end;

implementation

end.