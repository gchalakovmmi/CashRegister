  Interfaces.Model.Pattern.Observer,
  Interfaces.GUIRecords,
  Interfaces.Model.CashIn;

type
  IViewModelCashIn = interface
  ['{1929225C-798B-42DE-A754-B4ACE79A2A0D}']
