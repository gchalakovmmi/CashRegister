    ClassID:   '{8A22F25B-3E9A-4B47-86DC-B922CE424DD7}';
    IntfIID:   '{6182798D-B4CD-439A-A30F-CC86E6DFF201}';
    EventIID:  '';
    LicenseKey: nil;
    Version: 500);
begin
  ServerData := @CServerData;
end;

procedure TCSFP3530.Connect;
var
  punk: IUnknown;
begin
  if FIntf = nil then
  begin
    punk := GetServer;
    Fintf:= punk as ICSFP3530;
  end;
end;

procedure TCSFP3530.ConnectTo(svrIntf: ICSFP3530);
begin
  Disconnect;
  FIntf := svrIntf;
end;

procedure TCSFP3530.DisConnect;
begin
  if Fintf <> nil then
  begin
    FIntf := nil;
  end;
end;

function TCSFP3530.GetDefaultInterface: ICSFP3530;
begin
  if FIntf = nil then
    Connect;
  Assert(FIntf <> nil, 'DefaultInterface is NULL. Component is not connected to Server. You must call "Connect" or "ConnectTo" before this operation');
  Result := FIntf;
end;

