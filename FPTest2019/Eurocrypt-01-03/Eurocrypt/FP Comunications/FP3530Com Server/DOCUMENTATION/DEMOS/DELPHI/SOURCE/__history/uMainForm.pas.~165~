						dt_FP60_KL: Result := FP60_KL.CMD_49_5_1(GetParam(MY_PARAMS, 1), GetParam(MY_PARAMS, 2), GetParam(MY_PARAMS, 3), GetParam(MY_PARAMS, 4));
