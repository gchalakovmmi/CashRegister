			dt_WP500_KL: tmp_Int := WP_500_KL.command_054(TRGT_TEXT, ErrorCode);
			dt_FMP350_KL: tmp_Int := FMP_350KL.command_054(TRGT_TEXT, ErrorCode);
		end;
		if tmp_Int <> 0 then
